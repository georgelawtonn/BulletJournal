<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.Scene?>
<?import javafx.scene.text.Font?>
<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="253.0" prefWidth="325.0">
        <children>
            <VBox layoutX="24.0" layoutY="14.0" prefHeight="234.0" prefWidth="277.0" spacing="5.0">
                <children>
                    <HBox alignment="CENTER" prefHeight="28.0" prefWidth="329.0">
                        <children>
                            <Label prefHeight="23.0" prefWidth="155.0" text="Notes &amp; Quotes">
                                <font>
                                    <Font name="Courier New" size="18.0"/>
                                </font>
                            </Label>
                        </children>
                    </HBox>
                    <HBox prefHeight="145.0" prefWidth="277.0">
                        <children>
                            <TextArea fx:id="text" prefHeight="136.0" prefWidth="277.0"/>
                        </children>
                    </HBox>
                    <HBox alignment="CENTER" prefHeight="37.0" prefWidth="277.0" spacing="60.0">
                        <children>
                            <Button fx:id="submitQuotes" mnemonicParsing="false" text="Add to Quotes"/>
                            <Button fx:id="submitNotes" mnemonicParsing="false" text="Add to Notes"/>
                        </children>
                    </HBox>
                </children>
            </VBox>
        </children>
    </AnchorPane>
</Scene>
